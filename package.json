{
  "name": "jelurida",
  "version": "1.0.0",
  "description": "front-end project for emeka ",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass ./SASS/main.scss ./main.style.css -w",
    "compile:sass": "node-sass ./frontend/SASS/main.scss ./main.style.css",
    "concat:css": "concat -o ./main.style.css ./main.style.css ./main.style.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ./starter/css/style.concat.css -o ./starter/css/style.prefix.css",
    "compress:css": "node-sass ../main.style.prefix.css ../main.style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [
    "health",
    "care"
  ],
  "author": "veekthorr",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "^5.0.0"
  }
}
